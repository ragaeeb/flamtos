    },
    "dependencies": {
        "cli-welcome": "^3.0.1",
        "music-metadata": "^11.10.1"
    },
    "description": "CLI for interacting with Telegram platform.",
    "devDependencies": {
        "@biomejs/biome": "^2.3.5",
        "@types/bun": "^1.3.2",
        "@types/node": "^24.10.1",
        "semantic-release": "^25.0.2",
        "tsdown": "^0.16.4"
    },
    "engines": {
        "bun": ">=1.3.2",
        "node": ">=22.0.0"
    },
    "files": [
        "dist/*"
    ],
    "keywords": [
        "nodejs",
        "rename",
        "media",
        "automation"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "name": "flamtos",
    "scripts": {
        "build": "tsdown --config tsdown.config.ts",
